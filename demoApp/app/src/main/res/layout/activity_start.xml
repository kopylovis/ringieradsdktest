<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/gray">

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <FrameLayout
                android:id="@+id/asShowLogger"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="60dp"
                android:background="@color/white">
                <TextView
                    android:id="@+id/asLoggerTextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Show log overlay"
                    android:textColor="@color/black"
                    android:textSize="17dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/asSendEvents"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="1dp"
                android:background="@color/white">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Send pending events"
                    android:textColor="@color/black"
                    android:textSize="17dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/asGiveConsent"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="16dp"
                android:background="@color/white">
                <TextView
                    android:id="@+id/asConsentTextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Give consent"
                    android:textColor="@color/black"
                    android:textSize="17dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/asConsentType"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="1dp"
                android:background="@color/white">
                <TextView
                    android:id="@+id/asConsentTypeTextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Consent type: TCF v2"
                    android:textColor="@color/black"
                    android:textSize="17dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/acSetCustomProperty"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="16dp"
                android:background="@color/white">
                <TextView
                    android:id="@+id/asCustomPropertyTextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Set custom property: (testProperty:testPropertyValue)"
                    android:textColor="@color/black"
                    android:textSize="17dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/acSetMaxQueueSize"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="16dp"
                android:background="@color/white">
                <TextView
                    android:id="@+id/asSetMaxQueueSizeTextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Set max queue size"
                    android:textColor="@color/black"
                    android:textSize="17dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/acSetDispatchTimer"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="1dp"
                android:background="@color/white">
                <TextView
                    android:id="@+id/asSetDispatchTimer"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Set dispatch timer"
                    android:textColor="@color/black"
                    android:textSize="17dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/acDisableSdk"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="16dp"
                android:background="@color/white">
                <TextView
                    android:id="@+id/asDisableTextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Disable SDK"
                    android:textColor="@color/black"
                    android:textSize="17dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/acLogin"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="16dp"
                android:background="@color/white">
                <TextView
                    android:id="@+id/acLoginTextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Login"
                    android:textColor="@color/black"
                    android:textSize="17dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/acLogout"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="1dp"
                android:background="@color/white">
                <TextView
                    android:id="@+id/acLogoutTextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Logout"
                    android:textColor="@color/black"
                    android:textSize="17dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/asArticles"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="40dp"
                android:background="@color/white">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Articles"
                    android:textColor="@color/black"
                    android:textSize="17dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/asAudiences"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@color/white">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Get audiences"
                    android:textColor="@color/black"
                    android:textSize="17dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"/>
            </FrameLayout>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>